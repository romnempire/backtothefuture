extends layout

block content
  .navbar.navbar-inverse.navbar-fixed-top
    .navbar-header
      button(type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse")
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.navbar-brand(href="/") Home 

    .collapse.navbar-collapse.navbar-ex1-collapse
      ul.nav.navbar-nav
        li.active
          a(href="/tutorial") Tutorial 
        li
          a(href="/resources") Resources 
        li
          a(href="about") About
      form.navbar-form.navbar-right(role="search")
        .form-group
          input.form-control(type="text" placeholder="Search")
          button.btn.btn-default Submit

  a#time_is_link(href='http://time.is/New_York', rel='nofollow', style='font-size:36px;color:black;')
    span#New_York_z161(style='font-size:36px;color:black;')
    script(src='http://widget.time.is/t.js')
    script.
      time_is_widget.init({New_York_z161:{time_format:"12hours:minutes:secondsAMPM"}});

  
  form#checkout(method="post", action="/checkout")
    div#payment-form
    input(type="submit" value="Pay $10")

  script(src="https://js.braintreegateway.com/v2/braintree.js")
  script.
    var clientToken = "";

    braintree.setup(clientToken, "dropin", {
      container: "payment-form"
    });
  
